#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liuyue/Desktop/Fe/project/retypeWritter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/liuyue/Desktop/Fe/project/retypeWritter/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+ts-jest@28.0.5_lt5llx2ti6zt4eclhanfli5ynu/node_modules/ts-jest/cli.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/registry.npmmirror.com+ts-jest@28.0.5_lt5llx2ti6zt4eclhanfli5ynu/node_modules/ts-jest/cli.js" "$@"
fi
